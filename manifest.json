{
    "name": "Fast submission",
    "version": "1.0.1",
    "manifest_version": 3,
    "description": "ソースコードの提出をCtrl+Enterで出来るようになります (AtCoder/TOPSIC)",
    "icons": {
        "32": "submit.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://atcoder.jp/contests/*/tasks/*",
                "https://*/atcoder_problems/abc*",
                "https://*/examinations/*/problem_detail/*",
                "https://*/examinations/*/examination_results/*/resubmit"
            ],
            "js": ["content.js"]
        }
    ]
}
