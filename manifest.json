{
    "name": "TOPSIC - Ctrl+Enter submission",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "ソースコードの提出をCtrl+Enterで出来るようになります",
    "content_scripts": [
        {
            "matches": [
                "https://*/atcoder_problems/abc*",
                "https://*/examinations/*/examination_results/*/resubmit",
                "https://*/sql_drill/*"
            ],
            "js": ["content.js"]
        }
    ]
}
